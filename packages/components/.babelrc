{
  "env": {
    "test": {
      "presets": ["env", "react", "es2015"]
    },
    "development": {
      "presets": ["env", "react", "es2015"],
      "plugins": [
        "add-module-exports",
        "transform-object-rest-spread"
      ]
    },
    "production": {
      "presets": ["env", "react", "es2015", "minify"],
      "plugins": [
        "add-module-exports",
        "transform-object-rest-spread"
      ]
    }
  }
}
